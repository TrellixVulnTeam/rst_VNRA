#!/opt/rst/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'rst','console_scripts','rst'
__requires__ = 'rst'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('rst', 'console_scripts', 'rst')()
    )
